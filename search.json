---
---
[
  {% for card in site.posts %}
    {
      "title" : "{{ card.title | escape }}",
      "url" : "{{ site.baseurl | escape }}{{ card.url | escape }}",
      "caregories" : "{{ card.categories | join: ", " }}",
      "tags" : "{{ card.tags | join: ", " }}",
      "scarcity" : "{{ card.scarcity }}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
